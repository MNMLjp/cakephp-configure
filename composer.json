{
    "name": "mnmljp/cakephp3-configure",
    "description": "Configure plugin for CakePHP",
    "homepage": "https://github.com/MNMLjp/cakephp3-configure",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "cakephp/cakephp": "^3.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7.14|^6.0"
    },
    "autoload": {
        "psr-4": {
            "ConfigureTrait\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ConfigureTrait\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
